<!DOCTYPE html>
<html>
<head>
  <title>Workspace</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<!--settings-->
<!-- the modal is placed outside the navbar to allow it to appear above everything else -->
<div id="settings" class="modal">
  <div class="modal-content">
    <h4>Settings</h4>
    <form id="settings-form" class="col s12">
      <div class="row">
        <div class="input-field col s6">
          <i class="mdi-action-account-circle prefix"></i>
          <input placeholder="Mohamed" id="first_name" type="text" class="validate">
          <label for="first_name" class="active">First Name</label>
        </div>
        <div class="input-field col s6">
          <input placeholder="Abo El Soud" id="last_name" type="text" class="validate">
          <label for="last_name" class="active">Last Name</label>
        </div>
      </div>
      <div class="row">
        <div class="input-field col s12">
          <i class="mdi-communication-phone prefix"></i>
          <input id="icon_telephone" type="tel" class="validate">
          <label for="icon_telephone">Telephone</label>
        </div>
      </div>
      <div class="row" id="password-input">
        <div id="shown" class="input-field col s12">
          <input id="password" placeholder="(unchanged)" type="password" class="validate">
          <label for="password" class="active">Password</label>
        </div>
        <div id="hidden" class="input-field col s12 hide">
          <input id="password" type="password" class="validate">
          <label for="password" class="active">Enter the password again</label>
        </div>
      </div>
      <div class="row">
        <div class="input-field col s12">
          <input id="email" placeholder="mohamed.aaes@gmail.com" type="email" class="validate">
          <label for="email" class="active">Email</label>
        </div>
      </div>
    </form>
  </div>
  <div class="modal-footer">
    <a href="#!" id="save-button" class=" modal-action modal-close waves-effect waves-green btn-flat">Save</a>
    <a href="#!" id="cancel-button" class=" modal-action modal-close waves-effect waves-green btn-flat">Cancel</a>
  </div>
</div>
<!--end settings-->
